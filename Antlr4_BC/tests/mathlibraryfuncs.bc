/* testing the cos func */
x=90;
y=0;
z=180;
c(x);       /* should print -.44807361612917015236 */
c(y);       /* should print  1.000000000000000000  */    
c(z);       /* should print -.59846006905785813896 */


/* testing the natural log func */
x=21.75;
y=3.8;
z=-2.1;
l(1);       /* should print 0 */
l(0);       /* should print -Infinity */
l(x);       /* should print 3.07961375753469309974 */
l(y);       /* should print 1.33500106673234008540 */
l(z);       /* should print NaN, not a number */

/* testing the e exponential func */
a=15;
b=5+5;
c=-9.5;
e(1);       /* should print 2.71828182845904523536 */
e(0);       /* should print 1.00000000000000000000 */
e(a);       /* should print 3269017.37247211063930185504 */
e(b);       /* should print 22026.46579480671651695790 */
e(c);       /* should print 7.485182988770059 E-5 */
